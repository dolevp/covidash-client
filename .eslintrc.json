{
    "env": {
        "browser": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2020,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/prop-types": [0],
        "semi": [2, "never"],
        "no-restricted-syntax": [0],
        "no-param-reassign": [0],
        "react/jsx-props-no-spreading": [0],
        "no-lone-blocks": [0]
    }
}
